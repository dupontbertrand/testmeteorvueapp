preventDefault()\n      console.log('change');\n      var zipCode = this.form.zipCode;\n      console.log(zipCode);\n      axios.get('https://api-adresse.data.gouv.fr/search/?q=' + zipCode + '&limit=15', {\n      }).then(resp => {\n        console.log(resp)\n        var i = resp.data.features;\n        this.locations = i.map(f => {\n          console.log(f);\n          if (f.properties.type == \"municipality\") return f.city;\n          // return f.title;\n        })\n        console.log(this.locations);\n      }).catch(err => {\n        console.log(err)\n      })\n    }\n  },\n}\n"]},"sourceType":"module","hash":"1b5c2b0d245646064fced35a39dc4da2cc8df328"}
